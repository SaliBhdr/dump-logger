  "type": "package",
  "license": "MIT",
  "keywords": [
    "salibhdr",
    "laravel",
    "symfony",
    "logger",
    "log",
    "php",
    "simple logger",
    "pretty logger",
    "eye friendly logger",
    "dump logger"
  ],
  "authors": [
    {
      "name": "Salar Bahador",
      "email": "bahador.salar@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "symfony/var-dumper": ">=3.0"
  },
  "autoload": {
    "psr-4": {
      "SaliBhdr\\DumpLog\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}